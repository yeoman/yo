{
  "name": "yo",
  "version": "1.1.2",
  "description": "CLI tool for scaffolding out Yeoman projects",
  "license": "BSD",
  "bin": {
    "yo": "cli.js"
  },
  "repository": "git://github.com/yeoman/yo.git",
  "homepage": "http://yeoman.io",
  "author": "The Yeoman Team",
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
