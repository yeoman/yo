    "update-notifier": "~0.1.3",
    "insight": "~0.3.0",
    "sudo-block": "~0.3.0",
    "async": "~0.2.9",
    "open": "0.0.4",
    "chalk": "~0.4.0",
    "findup": "~0.1.3",
    "shelljs": "~0.2.6"
  },
  "peerDependencies": {
    "grunt-cli": ">=0.1.7",
    "bower": ">=0.9.0"
  },
  "devDependencies": {
    "grunt": "~0.4.2",
    "mockery": "~1.4.0",
